{
  "name": "react-lightning-design-system",
  "version": "0.1.21",
  "description": "Salesforce Lightning Design System components built with React",
  "main": "lib/scripts/index.js",
  "directories": {
    "example": "examples"
  },
  "keywords": [
    "react",
    "react-component",
    "salesforce",
    "lightning",
    "lightning design system",
    "slds",
    "tab",
    "form",
    "datepicker",
    "modal"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "lint": "eslint --ext .js src/**",
    "lint:examples": "eslint --ext .js examples/client/**",
    "watch": "watch 'npm run build' src/",
    "build": "babel --optional runtime,es7.objectRestSpread -d lib/ src/"
  },
  "files": [
    "lib"
  ],
  "author": "Shinichi Tomita <shinichi.tomita@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "classnames": "^2.1.3",
    "moment": "^2.10.6",
    "svg4everybody": "^2.0.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-eslint": "^4.1.6",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.13.1",
    "watch": "^0.16.0"
  }
}
